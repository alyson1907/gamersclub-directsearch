{
  "manifest_version": 2,
  "name": "GamersClub Direct Search",
  "description": "",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "popup.html",
  "options_ui": {
    "page": "popup.html"
  },
  "browser_action": {
    "default_icon": "assets/favicon.ico",
    "default_popup": "popup.html",
    "default_title": "GC-DirectSearch"
  },
  "background": {
    "scripts": [
      "directSearch.js"
    ]
  }
}
